#! /usr/bin/env VERBOSA=0 zsh

function __update_() {
    (conda update conda -y 1>/dev/null)
    (conda update --all -y 1>/dev/null)
    (yarn global add yarn@latest 1>/dev/null)
    (yarn global add npm@latest 1>/dev/null)
    (yarn global add pnpm@latest 1>/dev/null)
    (yarn global add typescript@latest 1>/dev/null)
    (yarn global add ts-node@latest 1>/dev/null)
}
__update_ &
exit 0
